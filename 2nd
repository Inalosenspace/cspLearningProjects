            byte n = Convert.ToByte(Console.ReadLine());
            if(n>=1 && n <= 100)
            {
                if (n % 2 == 1) Console.WriteLine("Weird");
                else if (n >= 2 && n <=5) Console.WriteLine("Not Weird");
                else if (n >= 6 && n <= 20) Console.WriteLine("Weird");
